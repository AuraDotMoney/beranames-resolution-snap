{
  "name": "beranames_resolver",
  "version": "1.0.1",
  "description": "This plugin includes the npm package to install as snap on metamask to resolve BNS - Berachain Name Service",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Beranames/beranames-resolution-snap.git"
  },
  "license": "MIT-0",
  "main": "./dist/bundle.js",
  "files": [
    "dist/",
    "snap.manifest.json",
    "images/beranamesLogo.svg"
  ],
  "scripts": {
    "allow-scripts": "yarn workspace root allow-scripts",
    "build": "mm-snap build",
    "build:clean": "yarn clean && yarn build",
    "clean": "rimraf dist",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "prepublishOnly": "mm-snap manifest",
    "serve": "mm-snap serve",
    "start": "mm-snap watch",
    "test": "jest"
  },
  "dependencies": {
    "@metamask/snaps-sdk": "9.3.0",
    "viem": "^2.22.8"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.0",
    "@metamask/auto-changelog": "^5.0.0",
    "@metamask/eslint-config": "^14.0.0",
    "@metamask/eslint-config-jest": "^14.0.0",
    "@metamask/eslint-config-nodejs": "^14.0.0",
    "@metamask/eslint-config-typescript": "^14.0.0",
    "@metamask/snaps-cli": "^8.0.0",
    "@metamask/snaps-jest": "^9.0.0",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-jest": "^29.0.0",
    "eslint-plugin-jsdoc": "^56.0.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^30.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.2.11",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.0",
    "typescript": "^4.7.4"
  },
  "packageManager": "yarn@3.8.7",
  "engines": {
    "node": ">=18.6.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
